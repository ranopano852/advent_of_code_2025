(text).count(')')
print(f" {text.count('(')-text.count(')')}")

floor = 0

for i in range(len(text)):
    if floor==-1:
        print(i)
        break
    elif text[i]=='(':
        floor+=1
    elif text[i]==')':
        floor-=1
